    "stylelint-config-tailwindcss": "^0.0.6",
    "stylelint-prettier": "^2.0.0",
    "tailwind-config-viewer": "^1.5.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "typescript": "^4.9.4",
