    "vue3-virtual-scroller": "^0.2.1",
    "vuex": "^4.0.2",
    "walletlink": "^2.1.5",
    "web3-utils": "^1.3.1",
    "worker-loader": "^3.0.8"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix --max-warnings 1",
    "*.{md,json,yaml,html}": "prettier --write",
    "*.{css,vue}": "stylelint --fix"
  },
