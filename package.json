{
  "name": "@balancer-labs/frontend-v2",
  "version": "1.75.0",
  "engines": {
    "node": "14.x",
    "npm": ">=7"
  },
  "scripts": {
    "serve": "NODE_ENV=development npm run generate:tokenlists && vue-cli-service serve",
    "build": "npm run generate:tokenlists && vue-cli-service build",
    "test:unit": "NODE_ENV=test vue-cli-service test:unit --cache --detectOpenHandles",
    "lint": "eslint . --ext .js,.ts,.vue --max-warnings 0",
    "import-abis": "cp ../core/deployments/artifacts/dockerParity/*.json ./src/utils/balancer/abi/.",
    "import-config": "tsc ./scripts/update-config.ts && node ./scripts/update-config.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check '**/*.*(md|json|yaml|html)' --loglevel warn",
    "prettier:fix": "npm run prettier -- --write",
    "prepare": "node ./prepare.js",
    "stylelint": "stylelint '**/*.{vue,css}'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "tailwind:build": "tailwind-config-viewer export ./tailwind-static",
    "tailwind:view": "tailwind-config-viewer -o",
    "generate:tokenlists": "ts-node -O '{\"module\": \"commonjs\"}' ./src/lib/scripts/tokenlists.generator.ts",
    "generate:voting-gauges": "ts-node -O '{\"module\": \"commonjs\"}' ./src/lib/scripts/voting-gauge.generator.ts",
    "generate:global-types": "ts-node -O '{\"module\": \"commonjs\"}' ./src/lib/scripts/global-component-types.generator.ts",
    "typecheck": "npm run generate:global-types && vue-tsc --noEmit"
  },
