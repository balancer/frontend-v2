    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.5",
    "vite-node": "^0.25.8",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-node-stdlib-browser": "^0.1.1",
    "vite-plugin-sentry": "^1.1.6",
    "vitest": "^0.27.0",
    "vitest-mock-extended": "^1.0.8",
